{
  "name": "metal-modal",
  "version": "0.0.0",
  "description": "A Modal component built using JSX templates",
  "license": "BSD",
  "repository": "metal/metal-modal",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "src/Modal.js",
  "files": [
    "src",
    "test"
  ],
  "scripts": {
    "test": "gulp test"
  },
  "keywords": [
    "metal"
  ],
  "babel": {
    "plugins": [
      [
        "incremental-dom",
        {
          "components": true,
          "prefix": "IncrementalDOM",
          "runtime": "iDOMHelpers"
        }
      ]
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal-jsx": "^1.0.0-rc.8"
  },
  "devDependencies": {
    "babel-plugin-incremental-dom": "^3.1.0",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0"
  }
}
